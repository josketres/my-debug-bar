{
  "name": "debugapp",
  "version": "1.0.0",
  "description": "A simple web-based debug application",
  "main": "main.js",
  "scripts": {
    "serve": "$(npm bin)/http-server .",
    "build": "npm run build-installer && npm run build-app",
    "build-installer": "$(npm bin)/browserify -t [ babelify ] debug-installer/main.js -o debug-installer/bundle.js",
    "build-app": "$(npm bin)/browserify -t [ babelify ] debug-app/main.js -o debug-installer/bundle.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babelify": "^7.3.0",
    "browserify": "^13.1.0",
    "http-server": "^0.9.0",
    "react": "^15.3.0",
    "react-dom": "^15.3.0"
  }
}
